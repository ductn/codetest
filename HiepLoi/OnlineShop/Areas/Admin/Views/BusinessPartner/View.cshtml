@model Model.EF.BusinessPartner
@using ClsCommon;
@{
    ViewBag.Title = "Thông tin";
    Layout = "~/Areas/Admin/Views/Shared/ecab/_LayoutsPopup.cshtml";
}
@*@section header{
        Thông tin người dùng
    }*@
@section jsFooter{

    <script src="@CommonConstants.DomainName/assets/jscontroller/BusinessPartnerController.js"></script>
}
<script>
    function cancel() {
        window.location.href = '/Admin/BusinessPartner/Index';
    }
</script>
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading bg-green">
                <p class="m-b-0 text-white text-uppercase">Thông tin chung</p>
            </div>
            <div class="panel-body">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="form-group row">
                            <label class="control-label col-lg-3 col-sm-3">Tên ngân hàng</label>
                            <div class="col-lg-9 col-sm-9">
                                @ViewBag.BusinessPartner.TieuDe
                            </div>
                            <label class="control-label col-lg-3 col-sm-3">Url</label>
                            <div class="col-lg-9 col-sm-9">
                                @ViewBag.BusinessPartner.Url
                            </div>
                            <label class="control-label col-lg-3 col-sm-3">Slogan</label>
                            <div class="col-lg-9 col-sm-9">
                                @ViewBag.BusinessPartner.Slogan
                            </div>
                            <label class="control-label col-lg-3 col-sm-3">Số điện thoại</label>
                            <div class="col-lg-9 col-sm-9">
                                @ViewBag.BusinessPartner.SDT
                            </div>
                            <label class="control-label col-lg-3 col-sm-3">Email</label>
                            <div class="col-lg-9 col-sm-9">
                                @ViewBag.BusinessPartner.Email
                            </div>
                            <label class="control-label col-lg-3 col-sm-3">Ảnh đại diện</label>
                            <div class="col-lg-9 col-sm-9">
                                <img src="@CommonConstants.DomainName/@ViewBag.BusinessPartner.Image" width="120" height="80" />
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>
</div>
