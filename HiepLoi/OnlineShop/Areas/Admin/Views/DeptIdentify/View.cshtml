@model Model.EF.DeptIdentify

@{
    ViewBag.Title = "Thông tin";
    Layout = "~/Areas/Admin/Views/Shared/ecab/_LayoutsPopup.cshtml";
}
@*@section header{
        Thông tin người dùng
    }*@
@section jsFooter{

    <script src="/assets/jscontroller/DeptIdentifyController.js"></script>
}
<script>
    function cancel() {
        window.location.href = '/Admin/DeptIdentify/Index';
    }
</script>
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading bg-green">
                <p class="m-b-0 text-white text-uppercase">Thông tin chung</p>
            </div>
            <div class="panel-body">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="form-group row">
                            <label class="control-label col-lg-3 col-sm-3">Mã số</label>
                            <div class="col-lg-9 col-sm-9">
                                @ViewBag.DeptIdentify.Code
                            </div>
                            <label class="control-label col-lg-3 col-sm-3">Tên cơ quan cấp</label>
                            <div class="col-lg-9 col-sm-9">
                                @ViewBag.DeptIdentify.Name
                            </div>
                            <label class="control-label col-lg-3 col-sm-3">Ghi chú</label>
                            <div class="col-lg-9 col-sm-9">
                                @Html.Raw(@ViewBag.DeptIdentify.Desription)
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>
</div>
